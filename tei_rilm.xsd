          <xs:attributeGroup ref="ns1:rilmteiatt.datable.attributes"/>
          <xs:attributeGroup ref="ns1:rilmteiatt.naming.attributes"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="event">
    <xs:annotation>
      <xs:documentation>contains data relating to any kind of significant event associated with a person, place, or organization. [13.3.1. Basic Principles]</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:rilmteimodel.headLike"/>
        <xs:choice>
          <xs:group maxOccurs="unbounded" ref="ns1:rilmteimodel.pLike"/>
          <xs:group maxOccurs="unbounded" ref="ns1:rilmteimodel.labelLike"/>
