      <xs:attributeGroup ref="ns1:rilmteiatt.media.attribute.height"/>
      <xs:attributeGroup ref="ns1:rilmteiatt.media.attribute.scale"/>
      <xs:attributeGroup ref="ns1:rilmteiatt.resourced.attributes"/>
      <xs:attributeGroup ref="ns1:rilmteiatt.timed.attributes"/>
      <xs:attribute name="mimeType" use="required">
        <xs:annotation>
